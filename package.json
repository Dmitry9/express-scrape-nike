{
  "name": "express-scrape-nike",
  "version": "1.0.0",
  "description": "API using the Express, serving as an interface for scraping product information from Nike.com (US version), and providing real-time status updates to clients. This API will operate in full compliance with Nike's terms of service and legal standards.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "dotenv -- nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e .ts",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\"",
    "test": "jest --detectOpenHandles"
  },
  "jest": {
    "preset": "./jest-preset.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cheerio": "^0.22.35",
    "ajv": "^8.12.0",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsdom": "^23.0.1",
    "morgan": "^1.10.0",
    "puppeteer": "^21.6.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsdom": "^21.1.6",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.11",
    "@types/puppeteer": "^7.0.4",
    "@types/supertest": "^2.0.12",
    "chalk": "^5.3.0",
    "dotenv-cli": "^7.2.1",
    "esbuild": "^0.17.17",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
